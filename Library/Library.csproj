    <RootNamespace>CpfLibrary</RootNamespace>
    <AssemblyName>Cpf</AssemblyName>
    <AssemblyVersion>3.0.0.0</AssemblyVersion>
    <FileVersion>3.0.0.0</FileVersion>
    <PackageId>Cpf.NetStandard</PackageId>
    <Authors>Robson Araújo (forked from https://github.com/tallesl/net-Cpf)</Authors>
